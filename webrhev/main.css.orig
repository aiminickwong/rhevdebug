#top_bar {

	background-color: black;
	color: red;
	width: 100%;
	height: 50px;
	margin: 0;
	clear:both;
}

#left_side {

	float:left;
	background-color: blue;
	height: 90%;
	width:49%;

}

#right_side {

	float:right;
	background-color: green;
	height:90%;
	width:49%;
}

#tabs {
	height: 2em;
	padding:0;
	margin:0;
	border: none;
	width: 100%;
	position:relative;
	float: left;

}

#tabs ul {

	clear:left;
	float:left;
	list-style:none;
	position: relative;
	text-align: center;
	width:100%;
	margin:0;
	padding:0;
}

#tabs li {
	display: block;
	float:left;
	height:100%;
	width: 33%;
	margin: 0 2px 0 0;

}

#tabs li a {
	text-decoration: none;
	color:black;
	line-height: 50%;
	position: relative;
	top: 25%;
}